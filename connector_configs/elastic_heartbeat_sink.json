{
    "name": "elastic-heartbeat-sink",
    "config": {
        "connector.class":
        "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
        "tasks.max": "1",
        "topics": "heartbeat-topic",
        "connection.url": "http://elastic_container:9200",
	"key.converter": "org.apache.kafka.connect.storage.StringConverter",
	"value.converter": "org.apache.kafka.connect.converters.IntegerConverter",	
	"key.ignore": "false",
        "type.name": "heartbeat"
    }
}
